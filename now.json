{
  "version": 2,
  "builds": [{ "src": "index.js", "use": "@now/node-server" }],
  "routes": [
    {
      "src": ".*",
      "methods": ["GET", "POST", "OPTIONS"],
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Headers": "Origin, X-Requested-With, Content-Type, Accept",
        "Access-Control-Allow-Credentials": "true"
      },
      "dest": "/index.js",
      "continue": true
    },
      {
        "src": "/listen",
        "dest": "/index.js",
        "methods": ["GET"]
      },
      {
        "src": "/listen/(?<slug>[^/]+)",
        "dest": "/index.js",
        "methods": ["GET"]
      },
      {
          "src": "/playlist",
          "dest": "/index.js",
          "methods": ["GET"]
        },
        {
          "src": "/playlist/(?<slug>[^/]+)",
          "dest": "/index.js",
          "methods": ["GET"]
        },
        {
          "src": "/v2/listen",
          "dest": "/index.js",
          "methods": ["GET","POST"]
        },
        {
          "src": "/project",
          "dest": "/index.js",
          "methods": ["GET"]
        },
        {
          "src": "/project/(?<value>[^/]+)",
          "dest": "/index.js",
          "methods": ["POST","GET"]
        },
        {
          "src": "/v2/listen/(?<slug>[^/]+)",
          "dest": "/index.js",
          "methods": ["GET","POST","PATCH"]
        },
        {
          "src": "/signup",
          "dest": "/index.js",
          "methods": ["POST"]
        },
        {
          "src": "/user",
          "dest": "/index.js",
          "methods": ["GET"]
        },
        {
          "src": "/login",
          "dest": "/index.js",
          "methods": ["POST,GET"]
        },
        {
          "src": "/settings",
          "dest": "/index.js",
          "methods": ["GET"]
        },
        {
          "src": "/notifications",
          "dest": "/index.js",
          "methods": ["GET"]
        }
    ]
    
}